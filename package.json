{
  "title": "Fantasy Marketplace",
  "name": "online-marketplace",
  "version": "5.2.0",
  "description": "Smart, trustworthy, open, portable, safe and secure. Build amazing things and join the sustainable lightweight design revolution.",
  "main": "index.js",
  "scripts": {
    "preinstall": "(npm list nsp -g || npm install nsp -g)",
    "watch": "supervisor --extensions 'node,js,hbs,json' server.js",
    "test": "mocha && nsp check"
  },
  "config": {
    "email": "captain@eselr.com",
    "domains": [
      "eselr.com",
      "www.eselr.com"
    ],
    "ports": {
      "http": 8080,
      "https": 8443
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fantasyui-com/online-marketplace.git"
  },
  "keywords": [],
  "author": "Captain Fantasy <fantasyui.com@gmail.com> (http://fantasyui.com)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/fantasyui-com/online-marketplace/issues"
  },
  "homepage": "https://github.com/fantasyui-com/online-marketplace#readme",
  "dependencies": {
    "auto-sni": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "hbs": "^4.0.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "mocha": "^6.0.2",
    "moment": "^2.18.1",
    "product-catalog": "^1.0.4",
    "stripe": "^4.17.1",
    "uuid": "^3.0.1",
    "xss-filters": "^1.2.7"
  },
  "routes": [
    {
      "routeName": "home",
      "moduleName": "simple-render",
      "httpVerb": "get",
      "urlPath": "/",
      "viewId": "index",
      "errorVerbosity": 5,
      "handlerEvent": [],
      "viewModel": [
        {
          "name": "featuredProducts",
          "module": "featured-products",
          "begin": 0,
          "end": 6
        },
        {
          "name": "popularProducts",
          "module": "popular-products",
          "begin": 0,
          "end": 9
        }
      ]
    },
    {
      "routeName": "about",
      "moduleName": "simple-render",
      "httpVerb": "get",
      "urlPath": "/about",
      "viewId": "about",
      "errorVerbosity": 5,
      "handlerEvent": [],
      "viewModel": []
    },
    {
      "routeName": "products",
      "moduleName": "simple-render",
      "httpVerb": "get",
      "urlPath": "/products",
      "viewId": "products",
      "errorVerbosity": 5,
      "handlerEvent": [],
      "viewModel": [
        {
          "name": "browseProducts",
          "module": "browse-products"
        }
      ]
    },
    {
      "routeName": "product",
      "moduleName": "simple-render",
      "httpVerb": "get",
      "urlPath": "/product/:authorId/:productName",
      "viewId": "product",
      "errorVerbosity": 5,
      "dataWhitelist": [
        {
          "id": "authorId"
        },
        {
          "id": "productName"
        }
      ],
      "handlerEvent": [
        {
          "name": "test",
          "module": "./action/test"
        }
      ],
      "viewModel": [
        {
          "name": "selectedProduct",
          "module": "selected-product"
        },
        {
          "name": "relatedProducts",
          "module": "related-products",
          "begin": 0,
          "end": 9
        }
      ]
    },
    {
      "routeName": "charge",
      "moduleName": "stripe-charge",
      "httpVerb": "post",
      "urlPath": "/charge",
      "viewId": "charge",
      "errorVerbosity": 5,
      "handlerEvent": [],
      "viewModel": []
    },
    {
      "routeName": "download",
      "moduleName": "download-file",
      "httpVerb": "get",
      "urlPath": "/download",
      "errorVerbosity": 5,
      "handlerEvent": [],
      "viewModel": []
    }
  ]
}
